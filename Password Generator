import random
import string

print("Note - Maximum size of password is 71")

# letters=string.ascii_letters
ULetters='ABCDEFGHIJKLMNOPQRSTUVWXYZ'
LLetters='abcdefghijklmnopqrstuvwxyz'
nums='0123456789'
special='-+*%$#@!_'
maker=ULetters+LLetters+nums+special
length=int(input("Please enter length of password: "))
password="".join(random.sample(maker,length))
password
